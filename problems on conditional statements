  <script>
    let age=parseInt(prompt("enter age:"));
    let person=prompt("enter person is pass or fail:");
    let citizen=prompt("peroson is citizen or not:")
    if(age>=18){
        if(citizen=="yes"){
            document.write("eligible to vote....")
        }
        else if(person="pass"&&citizen!="yes"){
            document.write("eligible to vote...")
        }
        else{
            document.write("not eligible to vote...")
        }
    }
    else{
        document.write("not eligible to vote...")
    }
    
   </script>
